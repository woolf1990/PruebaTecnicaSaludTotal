                $('#' + id).trigger({ type: "keydown", which: 85 });
            } else {
                //$("label[for='" + id.replace("ContentPlaceHolder1_", "") + "']").fadeOut(500);
            }

        }
        function placeHolderSelect(id) {
            var place = $("#" + id).attr('data-original-title');

            $("label[for='" + id.replace("ContentPlaceHolder1_", "") + "']").text(place);
            $("label[for='" + id.replace("ContentPlaceHolder1_", "") + "']").fadeIn(500);

            if ($("#" + id).val().length > 0) {
                $("label[for='" + id.replace("ContentPlaceHolder1_", "") + "']").fadeIn(500);
            } else {
                $("label[for='" + id.replace("ContentPlaceHolder1_", "") + "']").fadeOut(500);
            }
        }

